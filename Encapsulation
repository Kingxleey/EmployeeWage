let basicSalary = 30_000;
let overtime = 10;
let rate = 20;
 function getWage (basicSalary, overtime, rate){
    return basicSalary + (overtime * rate);
 }

 let employee = {
    basicSalary: 30_000,
    overtime: 10,
    rate: 20,
    getWage: function (){
    return this.basicSalary + (this.overtime * this.rate);
 }
};
console.log(employee.getWage());